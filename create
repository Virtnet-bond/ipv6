dhclient -r -6 &> /dev/null &
dhclient -6 &> /dev/null &
echo "Acquire::ForceIPv4 "true";" > /etc/apt/apt.conf.d/99force-ipv4 &> /dev/null
ip6=$(curl ifconfig.me -6 -s) &> /dev/null &
echo  "---------------------------------------------------------------"
echo "IPv6 Address: $ip6" 
echo  "---------------------------------------------------------------"
echo ""
